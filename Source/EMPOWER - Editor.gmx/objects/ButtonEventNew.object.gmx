global.eventRef = string(global.totalEventCards);//creates a string of the curent TotalEventNum
global.eventName = "Event"+global.eventRef;//creates the Event+*text of string set above
ini_open(global.cardEventINI);//opens the event ini
ini_write_real(global.eventName,"Ref", global.totalEventCards);//writes the current TotalEventNum as the "Ref"
