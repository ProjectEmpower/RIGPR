            <string>if(global.locationEventNumber &gt; 0)//if there are events
{
    ini_open(global.locationINI); //opens the ini file
    ini_key_delete(global.tempLocation,"Event"+string(global.EventID)+"Week");//deletes the current location
    ini_key_delete(global.tempLocation,"Event"+string(global.EventID)+"Mythical");//deletes the current location
    ini_key_delete(global.tempLocation,"Event"+string(global.EventID)+"Type");//deletes the current location
    for(i = global.EventID; i &lt; global.locationEventNumber; i++)
    {
        currRef = string(i);
        eventName = "Event"+currRef;
        nextRef = string(i+1);
        nextEventName = "Event"+nextRef;

        if(ini_key_exists(global.tempLocation,nextEventName+"Week") and !ini_key_exists(global.tempLocation,eventName+"Week"))
        {
            tempMyth = ini_read_real(global.tempLocation,nextEventName+"Mythical",0);
            tempWeek = ini_read_real(global.tempLocation,nextEventName+"Week",0);
            tempType = ini_read_real(global.tempLocation,nextEventName+"Type",0);
            
            ini_write_real(global.tempLocation,eventName+"Mythical",tempMyth);
            ini_write_real(global.tempLocation,eventName+"Week",tempWeek);
            ini_write_real(global.tempLocation,eventName+"Type",tempType);
            ini_key_delete(global.tempLocation,nextEventName+"Week");//deletes the current location
            ini_key_delete(global.tempLocation,nextEventName+"Mythical");//deletes the current location
            ini_key_delete(global.tempLocation,nextEventName+"Type");
        }

    }
    if(global.EventID == global.locationEventNumber)
        global.EventID -= 1;
    global.locationEventNumber -= 1; //after running, this will decrease the total instance number by 1
    ini_write_real(global.tempLocation,"Number of Events",global.locationEventNumber);
    ini_close();
}
room = RoomLocationAdd;
</string>
